	"$abovewater" 0

	"$surfaceprop" "water"
	"$envmap" "env_cubemap"
	"$envmaptint" "[ 1.00 .89 .80 ]"
	"$envmapcontrast" "1"

	"%keywords" "BEE2"
	"%compileSlime" 1

	"$fogenable" 1
	"$fogcolor" "{72 70 10}"
	"$fogstart" 1
