"LightmappedGeneric"
{

	$basetexture "BEE2/p1/goo/beneath"
	$bumpmap "BEE2/p1/goo/goo_normal"
	$abovewater 0

	$surfaceprop "slime"

	"%keywords" "BEE2"
	"%compileSlime" 1

	$fogenable 1
	$fogcolor "{72 70 10}"
	$fogstart 1
	$fogend 100
	
	"Proxies"
	{
		"AnimatedTexture"
		{
			animatedtexturevar "$bumpmap"
			animatedtextureframenumvar "$bumpframe"
			animatedtextureframerate 21.00
		}
	}
}
